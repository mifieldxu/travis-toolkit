{
  "name": "@qiwi/travis-client",
  "description": "Travis CI client",
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {},
  "version": "1.2.0",
  "scripts": {
    "build_es6": "flow-remove-types src/ --out-dir dist/es6/",
    "build_es5": "BABEL_ENV=production babel src --out-dir dist/es5/",
    "build": "rm -rf dist && npm run build_es5 && npm run build_es6"
  },
